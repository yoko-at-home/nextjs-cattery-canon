{
  "name": "nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -l -c fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "env": "vc env pull .env.local",
    "prepare": "husky install"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.4",
    "@tailwindcss/typography": "0.5.9",
    "axios": "1.4.0",
    "date-fns": "2.30.0",
    "microcms-js-sdk": "2.4.0",
    "next": "13.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss-neumorphism": "0.1.0"
  },
  "devDependencies": {
    "@sendgrid/mail": "7.7.0",
    "@types/node": "18.16.18",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "autoprefixer": "10.4.14",
    "classcat": "5.0.4",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import-access": "1.1.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tailwindcss": "3.12.1",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "keen-slider": "6.8.5",
    "lint-staged": "13.2.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "react-faq-component": "1.3.4",
    "react-hooks-use-modal": "3.3.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn fix:eslint",
    "*": "yarn fix:prettier"
  }
}
