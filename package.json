{
  "name": "nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -l -c fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "env": "vc env pull .env.local",
    "prepare": "husky install"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/line-clamp": "0.4.4",
    "@tailwindcss/typography": "0.5.10",
    "axios": "1.6.2",
    "date-fns": "2.30.0",
    "dayjs": "^1.11.10",
    "microcms-js-sdk": "2.7.0",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss-neumorphism": "0.1.0"
  },
  "devDependencies": {
    "@sendgrid/mail": "7.7.0",
    "@types/node": "20.9.2",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "autoprefixer": "10.4.16",
    "classcat": "5.0.4",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import-access": "2.1.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "keen-slider": "6.8.6",
    "lint-staged": "15.1.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "react-faq-component": "1.3.4",
    "react-hooks-use-modal": "3.3.1",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn fix:eslint",
    "*": "yarn fix:prettier"
  }
}
